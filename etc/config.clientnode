start = "clientnode"  -- main script

harbor = 0

--gameworld端口
login_port = 9999
gate_port = 8888
debug_console_port = 7777

-- 路径设置
root = "./"
luaservice = root.."service/?.lua;"..root.."skynet/service/?.lua;"..root.."service/test/?.lua;"
